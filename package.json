  "description": "基于 react 16.x、antd 4.x、react-router-dom 5.x 封装的控制台组件库。",
  "homepage": "https://github.com/shhhplus/antd-console-components",
  "main": "index.js",
  "author": {
    "name": "shhhplus"
  },
  "bugs": {
    "url": "https://github.com/shhhplus/antd-console-components/issues"
  },
  "license": "MIT",
  "keywords": [
    "antd",
    "console",
    "admin",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shhhplus/antd-console-components.git"
  },
  "dependencies": {
    "@shhhplus/react-router-relative-link": "^0.1.7",
    "@shhhplus/timer.js": "0.0.2",
    "antd": "^4.1.0",
    "classnames": "^2.2.6",
