    "postcss-url": "^8.0.0",
    "prettier": "^2.0.2",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
  }
